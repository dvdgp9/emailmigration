{
    "name": "mail-migration/webapp",
    "description": "IMAP Email Migration Tool - Webapp para migrar correos entre servidores IMAP",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ddeboer/imap": "^1.21"
    },
    "require-dev": {
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MailMigration\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "prefer-stable": true,
        "sort-packages": true
    },
    "minimum-stability": "stable"
} 